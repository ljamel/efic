{
  "version": 2,
  "name": "detectivedex",
  "builds": [
    {
      "src": "src/main/webapp/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/src/main/webapp/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/src/main/webapp/js/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/src/main/webapp/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        }
      ]
    },
    {
      "source": "/js/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/css/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/src/main/webapp/index.html"
    }
  ]
}
